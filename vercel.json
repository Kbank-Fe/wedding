{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/(admin|404)($|/.*)",
      "destination": "/index.html"
    },
    {
      "source": "^/([^/.]+)$",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
